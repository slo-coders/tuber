{
  "name": "tuber",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "an app encouraging users to satisfy the 33% rule by tracking a user's meetups in which they act as a mentor, peer, or mentee",
  "main": "client/index.js",
  "scripts": {
    "postinstall": "webpack",
    "seed": "node script/seed.js",
    "build-watch": "webpack -w --mode=development",
    "start-watch": "nodemon server/index.js --ignore public --ignore client",
    "start:dev": "NODE_ENV='development' npm run webpack-watch & NODE_ENV='development' npm run start-watch",
    "start": "node server/index.js",
    "test": "NODE_ENV='test' jest \"./test/*.spec.js\"",
    "lint": "node_modules/.bin/eslint client server"
  },
  "contributors": [
    "Hugo Campos",
    "Buddy Galletti",
    "Sam Olivas",
    "Thomas Smith",
    "Stephanie Stampher"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-redux": "^3.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "axios": "^0.19.0",
    "connect-session-sequelize": "^6.0.0",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "heroku": "^7.29.0",
    "pg": "^7.12.1",
    "sequelize": "^5.18.1"
  }
}
