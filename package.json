{
  "name": "tuber",
  "version": "1.0.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "description": "an app encouraging users to satisfy the 33% rule by tracking a user's meetups in which they act as a mentor, peer, or mentee",
  "main": "client/index.js",
  "scripts": {
    "postinstall": "webpack",
    "build-watch": "webpack -w --mode=development",
    "seed": "node server/db/utils/seed.js",
    "seed:test": "NODE_ENV='test' node server/db/utils/seed.js",
    "start": "node server/index.js",
    "start-watch": "nodemon server/index.js --ignore public --ignore client",
    "start:dev": "npm run build-watch & NODE_ENV='development' npm run start-watch",
    "start:sam": "env NODE_ENV='development' npm run build-watch & env NODE_ENV='development' npm run start-watch",
    "test": "npm run seed:test & NODE_ENV='test' jest",
    "test-watch": "npm run test -- --watchAll",
    "lint": "node_modules/.bin/eslint client server",
    "test:sam": "env NODE_ENV='test'",
    "test-watch:sam": "npm run test:sam -- --watchAll",
    "prettier": "prettier --write --single-quote \"client/**/*.js\" \"server/**/*.js\" "
  },
  "contributors": [
    "Hugo Campos",
    "Buddy Galletti",
    "Sam Olivas",
    "Thomas Smith",
    "Stephanie Stampher"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "verbose": true,
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 12
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.4.0",
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-redux": "^3.0.1",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "supertest": "^4.0.2",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "axios": "^0.19.0",
    "connect-session-sequelize": "^6.0.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "heroku": "^7.29.0",
    "pg": "^7.12.1",
    "sequelize": "^5.18.1"
  }
}
